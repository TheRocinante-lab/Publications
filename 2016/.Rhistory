FDR<-function(pvals, FDR){
pvalss<-sort(pvals, decreasing=F)
m=length(pvalss)
cutoffs<-((1:m)/m)*FDR
logicvec<-pvalss<=cutoffs
postrue<-which(logicvec)
print(postrue)
k<-max(c(postrue,0))
cutoff<-(((0:m)/m)*FDR)[k+1]
return(cutoff)
}
